{
  "name": "nestjs-pino",
  "version": "1.4.0",
  "description": "Pino logger for NestJS",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --verbose",
    "build": "rm -rf ./dist && tsc --p tsconfig.build.json",
    "report": "cat ./coverage/lcov.info | coveralls",
    "example": "rm -rf ./dist && tsc && LOG_LEVEL=debug node dist/example/main"
  },
  "engineStrict": false,
  "engines": {
    "node": "^4.7 || >=6.9 || >=7.3 || >=8.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamolegga/nestjs-pino.git"
  },
  "keywords": [
    "pino",
    "nestjs",
    "nest.js",
    "nest",
    "logger"
  ],
  "author": "Oleg Repin <iamolegga@gmail.com> (http://github.com/iamolegga)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamolegga/nestjs-pino/issues"
  },
  "homepage": "https://github.com/iamolegga/nestjs-pino#readme",
  "dependencies": {
    "@types/pino-http": "^5.0.0",
    "express-ctx": "^0.1.1",
    "pino-http": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^6.6.7",
    "@nestjs/core": "^6.6.7",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-fastify": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^26.0.0",
    "@types/memorystream": "^0.3.0",
    "@types/node": "^16.4.1",
    "@types/supertest": "^2.0.8",
    "coveralls": "^3.0.6",
    "express": "^4.17.1",
    "jest": "^25.1.0",
    "memorystream": "^0.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "supertest": "^4.0.2",
    "ts-jest": "^25.0.0",
    "tslint": "^6.0.0",
    "typescript": "^3.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "testEnvironment": "node"
  }
}
